{
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "last 3 and_chr versions",
    "last 3 chrome versions",
    "last 3 opera versions",
    "last 3 ios_saf versions",
    "last 3 safari versions"
  ],
  "scripts": {
    "serve": "browser-sync start --server '.' --files '.' 'css/' --extensions 'html'",
    "dev": "mkdir -p ./dist && cp ./src/sw.js ./dist/ && parcel serve 'src/index.html'",
    "clean": "rm -rf ./.parcel-cache && rm -rf ./dist",
    "build": "mkdir -p ./dist && cp ./src/sw.js ./dist/ && parcel build 'src/index.html' --no-source-maps --no-cache"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.0.1",
    "@parcel/transformer-sass": "^2.0.1",
    "@parcel/transformer-webmanifest": "^2.0.1",
    "@types/editorjs__header": "^2.6.0",
    "@types/pdfmake": "^0.1.19",
    "@types/xxhashjs": "^0.2.2",
    "autoprefixer": "^10.4.0",
    "browserslist": "^4.18.1",
    "editorjs-table": "^1.4.10",
    "parcel": "^2.0.1",
    "postcss": "^8.4.4"
  },
  "dependencies": {
    "@bomdi/codebox": "^1.0.12",
    "@editorjs/editorjs": "^2.22.2",
    "@editorjs/header": "^2.6.2",
    "@editorjs/marker": "^1.2.2",
    "@editorjs/nested-list": "^1.0.2",
    "@editorjs/quote": "^2.4.0",
    "@frank-mayer/magic": "^1.1.9",
    "pdfmake": "^0.2.4",
    "simple-image-editorjs": "^1.4.0",
    "xxhashjs": "^0.2.2"
  }
}
